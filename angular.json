{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "admin-toolkit": {
      "root": "projects/admin-toolkit",
      "sourceRoot": "projects/admin-toolkit/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/admin-toolkit/tsconfig.lib.json",
            "project": "projects/admin-toolkit/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/admin-toolkit/src/test.ts",
            "tsConfig": "projects/admin-toolkit/tsconfig.spec.json",
            "karmaConfig": "projects/admin-toolkit/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/admin-toolkit/tsconfig.lib.json",
              "projects/admin-toolkit/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "admin-toolkit-widgets": {
      "root": "projects/admin-toolkit-widgets",
      "sourceRoot": "projects/admin-toolkit-widgets/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/admin-toolkit-widgets/tsconfig.lib.json",
            "project": "projects/admin-toolkit-widgets/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/admin-toolkit-widgets/src/test.ts",
            "tsConfig": "projects/admin-toolkit-widgets/tsconfig.spec.json",
            "karmaConfig": "projects/admin-toolkit-widgets/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/admin-toolkit-widgets/tsconfig.lib.json",
              "projects/admin-toolkit-widgets/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "plugin-trello": {
      "root": "projects/plugin-trello",
      "sourceRoot": "projects/plugin-trello/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/plugin-trello/tsconfig.lib.json",
            "project": "projects/plugin-trello/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/plugin-trello/src/test.ts",
            "tsConfig": "projects/plugin-trello/tsconfig.spec.json",
            "karmaConfig": "projects/plugin-trello/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/plugin-trello/tsconfig.lib.json",
              "projects/plugin-trello/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "admin-toolkit"
}